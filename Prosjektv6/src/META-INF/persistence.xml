<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
	<persistence-unit name="PersistenceUnit" transaction-type="JTA">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>myDataSource</jta-data-source>
		
		<class> model.Lecturer</class>
		<class> model.Survey</class>

		<class> model.answer.Answer</class>
		<class> model.answer.MultipleChoiceAnswer</class>
		<class> model.answer.TextAnswer</class>

 		<class> model.question.MultipleChoiceQuestion</class>
 		<class> model.question.Question</class>
 		<class> model.question.TextQuestion</class>
		</persistence-unit>
</persistence> 